{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap test/**/*.test.js",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "migrate:createdb": "npx sequelize db:create --env default",
    "migrate:dropdb": "npx sequelize db:drop --env default",
    "migrate:up": "npx sequelize db:migrate --env default",
    "migrate:status": "npx sequelize db:migrate:status --env default",
    "migrate:down": "npx sequelize db:migrate:undo --env default"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.9.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^4.6.0",
    "http-errors": "^1.8.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "tap": "^14.11.0"
  }
}
